{
  "services": [
    {
      "id": "grafana",
      "name": "grafana",
      "tags": [
        "frontend-link"
      ],
      "address": "localhost",
      "port": 40000,
      "meta": {
        "url": "http://localhost:40000/grafana/3000"
      },
      "check": {
        "http": "http://grafana:3000/api/health",
        "interval": "15s",
        "deregister_critical_service_after": "1m"
      }
    }
  ]
}