{
  "services": [
    {
      "id": "redisinsight",
      "name": "redisinsight",
      "tags": [
        "frontend-link"
      ],
      "address": "localhost",
      "port": 15540,
      "meta": {
        "url": "http://localhost:15540"
      },
      "check": {
        "http": "http://redisinsight:5540/api/health",
        "interval": "15s",
        "deregister_critical_service_after": "1m"
      }
    },
    {
      "id": "redis-rs-1-exporter",
      "name": "redis-rs-1-exporter",
      "tags": [
        "frontend-link"
      ],
      "address": "localhost",
      "port": 40000,
      "meta": {
        "url": "http://localhost:40000/redis-rs-1-exporter/9121/metrics"
      },
      "check": {
        "tcp": "redis-rs-1-exporter:9121",
        "interval": "15s",
        "deregister_critical_service_after": "1m"
      }
    },
    {
      "id": "redis-rs-2-exporter",
      "name": "redis-rs-2-exporter",
      "tags": [
        "frontend-link"
      ],
      "address": "localhost",
      "port": 40000,
      "meta": {
        "url": "http://localhost:40000/redis-rs-2-exporter/9121/metrics"
      },
      "check": {
        "tcp": "redis-rs-2-exporter:9121",
        "interval": "15s",
        "deregister_critical_service_after": "1m"
      }
    }
  ]
}